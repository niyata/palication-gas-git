{
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "Logger": true,
    "SpreadsheetApp": true,
    "DriveApp": true,
    "UrlFetchApp": true,
    "PropertiesService": true,
    "GmailApp": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars": ["warn"],
    "no-undef": "off",
    "semi": ["error", "always"]
  }
}
